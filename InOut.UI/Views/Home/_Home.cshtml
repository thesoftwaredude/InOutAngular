<!-- http://ios-checkboxes.awardwinningfjords.com/ -->

<div class="jumbotron">
    Whos is in who is out
</div>


<div class="container" ng-controller="officeController" ng-cloak>
    <div class="row" ng-repeat="employee in office">
        <div class="col-md-1">
            <img ng-src="{{employee.imageUrl}}" style="width: 50px; height: 50px;"/>
        </div>
        <div class="col-md-7">
            {{employee.name}}
        </div>
        <div class="col-md-4">
            <input type="checkbox" ng-checked="{{employee.inOffice}}"/>
        </div>
    </div>
</div>
